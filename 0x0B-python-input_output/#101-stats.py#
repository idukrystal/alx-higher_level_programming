#!/usr/bin/python3

def getdata(st, no):
    le = len(st)
    for i in range(le):
        if st[i] == '<':
            no -= 1
        if no == 0:
            for j in range(i, le):
                if st[j] == '>':
                    return st[i+1:
fs = 0
sc = {200: 0, 301: 0, 400: 0, 401: 0, 403: 0, 404: 0, 405: 0, 500: 0}

while True:
    for i in range(10):
        try:
            line = stdin.readline()
            fs += int(getdata(line, 4))
            sc[int(getdata(line, 3))] += 1
        except (KeyboardInterrupt):
            break
    print(f"File size: {fs}")
    for key in sc:
        if sc[key] != 0:
            print(f"{key}: {sc[key]}")
            sc[key] = 0
